#include player.p8
#include rooms.p8

debug_rooms = true
debug_player = true

f = 1 -- frame count

function _init()
	init_rooms()
	init_player()
end

function _update60()
	p1_update()
	update_room()
end

function _draw()
	cls() -- clear screen
	draw_room(debug_rooms)
	p1_draw(debug_player)

	f += 1
end
